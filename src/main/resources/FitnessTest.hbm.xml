<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dcu.datamodel.FitnessTest" table="FitnessTest" schema="dbo" catalog="SSSMSDemo">
        <id name="fitnessId">
            <column name="fitnessId" sql-type="int"/>
        </id>
        <property name="bodyFat">
            <column name="bodyFat" sql-type="int" not-null="true"/>
        </property>
        <property name="chinUps">
            <column name="chinUps" sql-type="int" not-null="true"/>
        </property>
        <property name="pushUps">
            <column name="pushUps" sql-type="int" not-null="true"/>
        </property>
        <property name="playerId">
            <column name="playerId" sql-type="int"/>
        </property>
        <property name="sprint">
            <column name="sprint" sql-type="int"/>
        </property>
        <property name="kmRun">
            <column name="kmRun" sql-type="int"/>
        </property>
        <property name="agility">
            <column name="agility" sql-type="int"/>
        </property>
        <property name="workLifestyle">
            <column name="workLifestyle" sql-type="int" not-null="true"/>
        </property>
        <property name="weeklyAverageTrainingCount">
            <column name="weeklyAverageTrainingCount" sql-type="int"/>
        </property>

    </class>
</hibernate-mapping>