<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dcu.datamodel.Manager" table="Manager" schema="dbo" catalog="SSSMSDemo">
        <id name="managerId">
            <column name="managerId" sql-type="int"/>
        </id>
        <property name="firstName">
            <column name="firstName" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="phoneNumber">
            <column name="phoneNumber" sql-type="java.lang.Long" not-null="true"/>
        </property>
        <property name="teamId">
            <column name="teamId" sql-type="int" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="java.lang.String" not-null="true"/>
        </property>
        <!--
        <property name="logInId">
            <column name="logInId" sql-type="int" not-null="true"/>
        </property>
        -->
    </class>
</hibernate-mapping>