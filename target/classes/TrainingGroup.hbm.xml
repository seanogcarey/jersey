<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dcu.datamodel.TrainingGroup" table="TrainingGroup" schema="dbo" catalog="SSSMSDemo">
        <id name="trainingGroupId">
            <column name="trainingGroupId" sql-type="int"/>
        </id>
        <property name="playerId">
            <column name="playerId" sql-type="int" not-null="true"/>
        </property>
        <property name="fitnessId">
            <column name="fitnessId" sql-type="int"/>
        </property>
        <property name="fitnessGroup">
            <column name="fitnessGroup" sql-type="sysname" not-null="true"/>
        </property>
        <property name="strengthGroup">
            <column name="strengthGroup" sql-type="sysname" not-null="true"/>
        </property>
        <property name="speedGroup">
            <column name="speedGroup" sql-type="sysname" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>