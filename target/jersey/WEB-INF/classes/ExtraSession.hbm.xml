<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dcu.datamodel.ExtraSession" table="ExtraSession" schema="dbo" catalog="SSSMSDemo">
        <id name="extraSessionId">
            <column name="extraSessionId" sql-type="int"/>
        </id>
        <property name="playerId">
            <column name="playerId" sql-type="int"/>
        </property>
        <property name="weekId">
            <column name="weekId" sql-type="int"/>
        </property>
        <property name="sessionType1">
            <column name="sessionType1" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType2">
            <column name="sessionType2" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType3">
            <column name="sessionType3" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType4">
            <column name="sessionType4" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType5">
            <column name="sessionType5" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType6">
            <column name="sessionType6" sql-type="java.lang.String" not-null="true"/>
        </property>
        <property name="sessionType7">
            <column name="sessionType7" sql-type="java.lang.String" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>